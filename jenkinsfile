pipeline {
    agent any
stage('init')
{
checkout scm
}
    stages {
        stage('Build')
        {
        withMaven(maven: 'mvn'){
            steps {
                sh "mvn clean install"
                echo 'Building..'
            }
        }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}

