pipeline {
    agent any
	{
    stages
	{
	stage('init')
{
checkout scm
}
        stage('Build')
        {
        
		 steps 
			{
			withMaven(maven: 'mvn') 
			{
                sh "mvn clean install"
                echo 'Building..'
            }
        }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}
}
